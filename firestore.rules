rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId}/likes/{movieId} {
      allow write: if request.auth.uid == userId;
      allow read: if request.auth.uid != null
    }
  }
  match /databases/{database}/documents {
    match /users/{userId}/watch/{movieId} {
      allow write: if request.auth.uid == userId;
      allow read: if request.auth.uid != null
    }
  }
}
